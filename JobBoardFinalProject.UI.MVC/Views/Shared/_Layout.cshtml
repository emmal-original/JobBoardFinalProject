                                                    <li>@Html.ActionLink("User Details", "Index", "UserDetails")</li>
                                                    <li>@Html.ActionLink("Applications", "Index", "Applications")</li>
                                                }

                                                @if (Request.IsAuthenticated && (User.IsInRole("Admin") || User.IsInRole("Manager")))
