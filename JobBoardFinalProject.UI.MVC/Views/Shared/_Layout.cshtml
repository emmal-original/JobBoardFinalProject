                                                    <li>@Html.ActionLink("Manage Open Positions", "Index", "ManageOpenPositions")</li>
                                                    <li>@Html.ActionLink("Manage Applications", "Index", "ManageApplications")</li>
                                                }

                                                @if (Request.IsAuthenticated && User.IsInRole("Admin"))
